{
	"name": "pastabin/app",
	"description": "PastaBin aims to be a modern and secure pastebin replacement!",
	"keywords": ["pastabin", "text sharing", "paste", "secure"],
	"homepage": "https://pastabin.pw",
	"license": "MIT",
	"authors": [
		{
			"name": "Ogromny",
			"email": "ogromnym@openmailbox.org"
		},
		{
			"name": "SkyzohKey",
			"email": "skyzohkey@protonmail.com"
		},
		{
			"name": "Contributors",
			"homepage": "https://pastabin.pw/contributors"
		}
	],
	"autoload": {
		"psr-4": {
			"app\\": "app"
		}
	},
	"require": {
		"php": ">=5.5.0",
		"mako/framework": "4.5.*"
	},
	"config": {
	    "preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			 "dev-master": "4.5-dev"
		}
	},
	"suggest": {
		"mako/toolbar": "Makes it easier to debug your application while its under development"
	}
}
